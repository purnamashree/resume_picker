<?xml version="1.0" encoding="UTF-8"?>
<web-app>
  <display-name>Archetype Created Web Application</display-name>
  <servlet>
		<servlet-name>LoginService</servlet-name>
		<servlet-class>com.sagarsoft.service.LoginService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>LoginService</servlet-name>
		<url-pattern>/logged_in</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>SuperAdminController</servlet-name>
		<servlet-class>com.sagarsoft.controller.SuperAdminController</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>SuperAdminController</servlet-name>
		<url-pattern>/super_admin_page</url-pattern>
	</servlet-mapping>
	
<!--	<servlet>
		<servlet-name>DeleteUserService</servlet-name>
		<servlet-class>com.sagarsoft.service.DeleteUserService</servlet-class>
	</servlet>
	  -->
	<servlet-mapping>
		<servlet-name>SuperAdminController</servlet-name>
		<url-pattern>/delete_user</url-pattern>
	</servlet-mapping>
	<servlet>
		<servlet-name>AddUserService</servlet-name>
		<servlet-class>com.sagarsoft.service.AddUserService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddUserService</servlet-name>
		<url-pattern>/load</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>UploadResumeService</servlet-name>
		<servlet-class>com.sagarsoft.service.UploadResumeService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>UploadResumeService</servlet-name>
		<url-pattern>/upload</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>ChangePasswordService</servlet-name>
		<servlet-class>com.sagarsoft.service.ChangePasswordService</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>ChangePasswordService</servlet-name>
		<url-pattern>/login_page</url-pattern>
	</servlet-mapping>
	
	
	<welcome-file-list>
		<welcome-file>login.jsp</welcome-file>
	</welcome-file-list>
</web-app>
